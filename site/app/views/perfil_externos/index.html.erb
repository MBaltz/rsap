<p id="notice"><%= notice %></p>

<h1>Perfil Externos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Link</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @perfil_externos.each do |perfil_externo| %>
      <tr>
        <td><%= perfil_externo.nome %></td>
        <td><%= perfil_externo.link %></td>
        <td><%= perfil_externo.user %></td>
        <td><%= link_to 'Ver', perfil_externo %></td>
        <td><%= link_to 'Editar', edit_perfil_externo_path(perfil_externo) %></td>
        <td><%= link_to 'Apagar', perfil_externo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Perfil Externo', new_perfil_externo_path %>
